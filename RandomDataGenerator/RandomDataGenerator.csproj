<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Geo.RandomNameGenerator</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Data\dist.all.last" />
    <None Remove="Data\dist.female.first" />
    <None Remove="Data\dist.male.first" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Data\dist.all.last">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Data\dist.female.first">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Data\dist.male.first">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
